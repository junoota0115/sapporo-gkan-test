<dl class="see">
  <dt><%= @model.t :name %></dt>
  <dd><%= @item.name %></dd>
</dl>

<% @crud_buttons = proc do |f| %>
  <%= f.submit t("ss.buttons.delete"), class: 'btn-danger delete', data: { disable_with: t("ss.buttons.deleting") } %>
  <%= f.button t("ss.buttons.cancel"), class: 'btn-default', type: :reset, onclick: "location.href='#{crud_redirect_url}'; return false;" %>
<% end %>
